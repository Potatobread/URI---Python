v = input()

if(v == 61):
  print('Brasilia')
elif(v == 71):
  print('Salvador')
elif(v == 11):
  print('Sao Paulo')
elif(v == 21):
  print('Rio de Janeiro')
elif(v == 32):
  print('Juiz de Fora')
elif(v == 19):
  print('Campinas')
elif(v == 27):
  print('Vitoria')
elif(v == 31):
  print('Belo Horizonte')
else:
  print('DDD nao cadastrado')
