valores = raw_input().split(" ")

a, b, c = valores

pi = 3.14159
trian = (float(a) * float(c)) / 2
circ = pi *(float(c) * float(c))
trap = (float(a) + float(b)) * float(c) / 2
quad = float(b) * float(b)
retan = float(a) * float(b)

print('TRIANGULO: %.3f\nCIRCULO: %.3f\nTRAPEZIO: %.3f\nQUADRADO: %.3f\nRETANGULO: %.3f' %(trian, circ, trap, quad, retan))
