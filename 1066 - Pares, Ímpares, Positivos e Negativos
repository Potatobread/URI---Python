n = 0
cont = cont2 = cont3 = cont4 = 0
while(n != 5):
  v = int(input())
  if(v % 2 == 0):
    cont += 1
  else:
    cont2 += 1
  
  if(v > 0):
    cont3 += 1
  elif(v < 0):
    cont4 += 1
  n += 1
  
print('%d valor(es) par(es)' %cont)
print('%d valor(es) impar(es)' %cont2)
print('%d valor(es) positivo(s)' %cont3)
print('%d valor(es) negativo(s)' %cont4)
